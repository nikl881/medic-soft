<ul class="list-unstyled">
    <div class="d-flex">
        <div class="fixed-width-120">Gender:</div>
        <div>n/a</div>
    </div>
    <div class="d-flex">
        <div class="fixed-width-120">Email:</div>
        <div>{{ patient.email }}</div>
    </div>
    <div class="d-flex">
        <div class="fixed-width-120">Birthdate:</div>
        <div>{{ patient.birthdate|date("d-m-Y") }}</div>
    </div>
    <div class="d-flex">
        <div class="fixed-width-120">Phonenumber:</div>
        <div>{{ patient.phoneNumber }}</div>
    </div>
    <div class="d-flex">
        <div class="fixed-width-120">Insurance No:</div>
        <div>{{ patient.insuranceNumber }}</div>
    </div>
    <div class="d-flex">
        <div class="fixed-width-120">Insurance Co:</div>
        <div>{{ patient.insuranceCompany }}</div>
    </div>
    <div class="d-flex">
        <div class="fixed-width-120">Active:</div>
        <label class="c-switch c-switch-sm c-switch-pill c-switch-primary">
            <input type="checkbox" class="c-switch-input" checked>
            <span class="c-switch-slider"></span>
        </label>
    </div>
</ul>